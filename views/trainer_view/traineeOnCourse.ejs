<div class="container-fluid">
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Trainees on Course</h6>
    </div>
    <div class="card-body">
      
      <div class="table-responsive">
        <table
          class="table table-bordered"
          id="dataTable4"
          width="100%"
          cellspacing="0"
        >
          <thead>
            <tr>
              
              <th scope="col">Trainee</th>
              
            </tr>
          </thead>
          <tbody>
            <% traineeCourses.forEach(function(traineeCourse) { %>
            <tr>
              
              <td><%= traineeCourse.Trainee.fullname %></td>
              
            </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>